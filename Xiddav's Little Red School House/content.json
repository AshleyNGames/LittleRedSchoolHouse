 	{  "Name": "School Built",  	"Value": "true",   	"When":    {"HasSeenEvent": "9695001"},},
 	{  "Name": "SVE",  				"Value": "false",   "When":    {},},
 	{  "Name": "SVE",  				"Value": "true",   	"When":    {"HasMod": "FlashShifter.StardewValleyExpandedCP"},},
 	{  "Name": "R3",  				"Value": "false",   "When":    {},},
